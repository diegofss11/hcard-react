{
  "name": "hcard-react",
  "author": "Diego Freitas (diegofss11@gmail.com)",
  "version": "1.0.0",
  "license": "MIT",
  "description": "HCard build in ReactJS",
  "private": true,
  "keywords": [
    "reactjs"
  ],
  "scripts": {
    "start": "npm run build",
    "test": "mocha --compilers js:babel-register --require ./tests/helpers.js --require ignore-styles ./tests/* --recursive",
    "tdd": "npm test -- --watch",
    "build": "webpack -d && cp src/index.html dist/index.html && webpack-dev-server --inline --hot",
    "build:prod": "webpack -p && cp src/index.html dist/index.html"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.3.0",
    "mocha": "^4.0.1",
    "node-sass": "^4.6.0",
    "sass-loader": "^6.0.6",
    "sinon": "^4.1.2",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "dependencies": {
    "react": "^16.1.0",
    "react-dom": "^16.1.0"
  }
}
